config_version=5


[application]

config/name="Circe's Garden"
run/main_scene="res://game/features/ui/main_menu.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GameState="*res://game/autoload/game_state.gd"
AudioController="*res://game/autoload/audio_controller.gd"
SaveController="*res://game/autoload/save_controller.gd"
SceneManager="*res://game/autoload/scene_manager.gd"
CutsceneManager="*res://game/autoload/cutscene_manager.gd"
Constants="*res://game/autoload/constants.gd"
MCPInputHandler="*res://addons/godot_mcp/mcp_input_handler.gd"

[display]

window/size/viewport_width=1080
window/size/viewport_height=1240
window/size/mode=2
window/size/resizable=false
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/GDQuest_GDScript_formatter/plugin.cfg", "res://addons/godot_mcp/plugin.cfg")


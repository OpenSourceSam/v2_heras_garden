config_version=5

[android]

#Android-specificsettings(precursorsforfutureAPKbuild)#Thesewillbeappliedwhenexport_presets.cfgisloadedinGodot#Packagenameformat:com.yourname.circesgardenpackage/name="com.yourname.circesgarden"
package/label="Circe's Garden"
package/signed=true
package/app_category=2
package/short_version="1.0.0"
package/version_code=1
package/min_sdk=24
package/target_sdk=34
package/exclude_from_recents=false
package/show_in_app_library=true
package/stl="c++_shared"
package/permissions=PackedStringArray("android.permission.INTERNET")
package/features=PackedStringArray("android.hardware.touchscreen")
orientation/portrait=false
orientation/landscape=true
orientation/reverse_landscape=false
orientation/sensor_landscape=false
orientation/sensor_portrait=false
orientation/reverse_portrait=false
window/immersive_mode=true
window/layout_direction=0
keep_screen_on=true

[application]

config/name="Circe's Garden"
run/main_scene="res://game/features/ui/main_menu.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
boot_splash/bg_color=Color(0.14117648, 0.14117648, 0.14117648, 1)
config/icon="res://icon.svg"

[autoload]

GameState="*res://game/autoload/game_state.gd"
AudioController="*res://game/autoload/audio_controller.gd"
SaveController="*res://game/autoload/save_controller.gd"
SceneManager="*res://game/autoload/scene_manager.gd"
CutsceneManager="*res://game/autoload/cutscene_manager.gd"
Constants="*res://game/autoload/constants.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
QuestSystem="*res://addons/quest_system/quest_manager.gd"
MCPInputHandler="*res://addons/godot_mcp/mcp_input_handler.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=2
window/size/resizable=false
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/BurstParticles2D/plugin.cfg", "res://addons/GDQuest_GDScript_formatter/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg", "res://addons/gdUnit4/plugin.cfg", "res://addons/godot_mcp/plugin.cfg", "res://addons/quest_system/plugin.cfg")


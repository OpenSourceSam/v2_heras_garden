id = "daedalus_intro"
lines = [{
"speaker": "Daedalus",
"text": "Hello? Is anyone here? I heard a witch lives on this island.",
"emotion": "hopeful"
}, {
"speaker": "Daedalus",
"text": "Ah, thank the gods. I am Daedalus - a craftsman, architect, and inventor.",
"emotion": "warm"
}, {
"speaker": "Daedalus",
"text": "My ship needs repairs. Storms battered us off course, and we sought shelter here.",
"emotion": "concerned"
}, {
"speaker": "Daedalus",
"text": "I can pay for supplies, or work in exchange. I built the Labyrinth for King Minos, back in Crete.",
"emotion": "proud"
}, {
"speaker": "Daedalus",
"text": "The woman here - Circe, was it? - she has offered me shelter while repairs are made.",
"emotion": "grateful"
}, {
"speaker": "Daedalus",
"text": "If you want to help her, ask her what she wants.",
"emotion": "gentle"
}, {
"speaker": "Daedalus",
"text": "Perhaps you could use an old craftsman's help?",
"emotion": "hopeful"
}]

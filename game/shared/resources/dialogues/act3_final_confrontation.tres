id = "act3_final_confrontation"
lines = [{"speaker": "Scylla", "text": "You return with another lie?"},
{"speaker": "Circe", "text": "I return with the last potion."},
{"speaker": "Circe", "text": "Let this end your torment."},
{"speaker": "Scylla", "text": "Then do it."},
{"speaker": "Circe", "text": "May this be mercy."},
{"speaker": "Scylla", "text": "You did it?", "emotion": "weary"},
{"speaker": "Circe", "text": "I did.", "emotion": "weak"},
{"speaker": "Scylla", "text": "To try another potion?", "emotion": "weary"},
{"speaker": "Circe", "text": "Yes. But... not to fix you. To set you free.", "emotion": "sad"},
{"speaker": "Scylla", "text": "Then why?", "emotion": "weary"},
{"speaker": "Circe", "text": "[holds up vial] To set you free.", "emotion": "sad"},
{"speaker": "Scylla", "text": "That's... that's a death potion.", "emotion": "weary"},
{"speaker": "Scylla", "text": "Finally.", "emotion": "weary"},
{"speaker": "Circe", "text": "I'm sorry. I'm so, so sorry for what I did to you.", "emotion": "guilty"},
{"speaker": "Circe", "text": "I was jealous, and cruel, and I destroyed your life.", "emotion": "guilty"},
{"speaker": "Circe", "text": "I can't undo it. But I can... end it.", "emotion": "sad"},
{"speaker": "Circe", "text": "If you want me to.", "emotion": "sad"}]
